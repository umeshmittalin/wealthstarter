{
  "openapi": "3.1.0",
  "info": {
    "title": "PriceQuote API",
    "version": "0.0.1",
    "description": "Return latest price for supported tickers/fund symbols. Replace with your internal microservice."
  },
  "servers": [
    {
      "url": "https://your-domain.example.com"
    }
  ],
  "paths": {
    "/v1/quote": {
      "get": {
        "summary": "Get latest price",
        "parameters": [
          {
            "name": "symbol",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "symbol": {
                      "type": "string"
                    },
                    "price": {
                      "type": "number"
                    },
                    "currency": {
                      "type": "string"
                    },
                    "as_of": {
                      "type": "string",
                      "format": "date-time"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}